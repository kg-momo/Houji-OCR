 <PAGE IMAGENAME="R0000014_contents_L.jpg">
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2384" STRING="第十九条一項この法律施行の際現に都道府県知事の免許、許可その他の処分を受けて理髪又は美容を業と" STR_CONF="0.969" TYPE="本文" WIDTH="57" X="1358" Y="746"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="887" STRING="〓一定期間を限り資格者とみなす場合" STR_CONF="0.934" TYPE="本文" WIDTH="52" X="1044" Y="650"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="1738" STRING="〓都道府県知事の免許等を受けて業を行なつている者を資格者とみなす場合" STR_CONF="0.640" TYPE="本文" WIDTH="53" X="1568" Y="650"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2024" STRING="〓一定の要件を備える者のうち、大臣が試験免除を相当と認めたものに資格を与える場合" STR_CONF="0.993" TYPE="本文" WIDTH="63" X="521" Y="656"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="965" STRING="髪師又は理容師の免許を受けた者とみなす。" STR_CONF="0.718" TYPE="本文" WIDTH="50" X="1154" Y="794"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2331" STRING="している者は、これを第二条〔理容師となるための要件〕又は第三条[美容師となるための要件〕の規定による理" STR_CONF="0.533" TYPE="本文" WIDTH="54" X="1253" Y="797"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="644" STRING="列調理師法(昭三三法一四七)" STR_CONF="0.346" TYPE="本文" WIDTH="46" X="948" Y="699"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2382" STRING="2この法律の施行の際、現に都道府県知事の免許による調理士又は調理師である者は、この法律施行後三" STR_CONF="0.416" TYPE="本文" WIDTH="58" X="829" Y="746"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="1254" STRING="資格〕の規定にかかわらず、調査士となる資格を有する。" STR_CONF="0.971" TYPE="本文" WIDTH="50" X="1887" Y="804"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2338" STRING="法務局の長の選考を受け、調査士となるにふさわしい知識及び技能を有すると認められたときは、第三条" STR_CONF="0.564" TYPE="本文" WIDTH="53" X="1989" Y="796"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="1160" STRING="年に限り、第三条第一項の免許を受けた者とみなす。" STR_CONF="0.849" TYPE="本文" WIDTH="54" X="730" Y="797"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2387" STRING="3昭和二十六年三月三十一日までに左の各号の一に該当する者は、同年六月三十日までに法務局又は地方" STR_CONF="0.487" TYPE="本文" WIDTH="54" X="2093" Y="748"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="649" STRING="列理容而法(昭二二法二三四)" STR_CONF="0.890" TYPE="本文" WIDTH="47" X="1471" Y="698"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2021" STRING="土地又は家屋に関する調査、測量又は申告手続に関し七年以上の実務の経験を有する者" STR_CONF="0.946" TYPE="本文" WIDTH="51" X="1781" Y="816"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="168" STRING="以下路)" STR_CONF="0.537" TYPE="本文" WIDTH="41" X="1686" Y="839"/>
