 <PAGE IMAGENAME="R0000014_contents_R.jpg">
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="1044" STRING="リ実務経検を有する者を資各者となす昜合" STR_CONF="0.001" TYPE="本文" WIDTH="54" X="1001" Y="647"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="607" STRING="リ了文〓二二((召二六去四)" STR_CONF="0.004" TYPE="本文" WIDTH="47" X="901" Y="698"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="992" STRING="〓了政庁の選考により資各者となす昜合" STR_CONF="0.031" TYPE="本文" WIDTH="56" X="472" Y="645"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="284" STRING="のの又及ヽ" STR_CONF="0.094" TYPE="本文" WIDTH="45" X="1216" Y="602"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2514" STRING="况行法規における附則では以上の問題についてどのような規定を設けているか以下順次例示してみることとする。" STR_CONF="0.482" TYPE="本文" WIDTH="66" X="1412" Y="549"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2532" STRING="貝格付与法規における附則の問題としては、立法者は、資格付与法規が新たに制定された場合、現に有資格者でな" STR_CONF="0.282" TYPE="本文" WIDTH="68" X="1829" Y="601"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="1338" STRING="〓新に制された法律上の資格を与えることとする易合" STR_CONF="0.008" TYPE="本文" WIDTH="75" X="1099" Y="599"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2325" STRING="の一に該当する者を除く。)で、同条に規定する業務を行つた年数を通算して三年以上になるものは、この" STR_CONF="0.521" TYPE="本文" WIDTH="68" X="669" Y="804"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2570" STRING="〓、或いは日法上の資格要件や登録その他の手続等についての経過的措置等を明らかにしなければならない。そこで" STR_CONF="0.244" TYPE="本文" WIDTH="66" X="1515" Y="556"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2390" STRING="nこの法律施行の際、現に第一条に規定する業務を行つている者(第五条〔欠格事由〕第一号から第四号まで" STR_CONF="0.043" TYPE="本文" WIDTH="60" X="781" Y="745"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2580" STRING="究された場合、旧法上の有資格者又は旧法上一定の資格要件を有する者について、これを新法上どのように取り扱う" STR_CONF="0.556" TYPE="本文" WIDTH="72" X="1615" Y="554"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2583" STRING="しれば行ない得ないこととなる行為を業としていた者の権益とか地位をどのように考えるか、又、資格付与法規が改" STR_CONF="0.195" TYPE="本文" WIDTH="69" X="1721" Y="553"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="2569" STRING="資格付与法規が新たに制定された場合にその制定前から当該業務を行なつている者及び一定の学歴、経験を有す" STR_CONF="0.589" TYPE="本文" WIDTH="68" X="1300" Y="568"/>
  <LINE CONF="1.000" DIRECTION="縦" HEIGHT="846" STRING="列二也〓量問一二去(召二五去二二)" STR_CONF="0.001" TYPE="本文" WIDTH="50" X="373" Y="692"/>
  <LINE CONF="0.999" DIRECTION="縦" HEIGHT="819" STRING="法律の見〓による了文書七こな4。" STR_CONF="0.000" TYPE="本文" WIDTH="52" X="579" Y="792"/>
